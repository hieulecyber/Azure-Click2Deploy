{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmCount": {
      "value": 1
    },
    "vmName": {
      "value": "vm"
    },
    "vmSize": {
      "value": "Standard_B2s"
    },
    "vmAdminUser": {
      "value": "azureuser"
    },
    "vmAdminPassword": {
      "value": null
    },
    "scriptName": {
      "value": "cse-vmGeneric.ps1"
    },
    "scriptUris": {
      "value": [
        "[format('https://raw.githubusercontent.com/jamesdplee/cloudlee-click2deploy/main/scripts/{0}', parameters('scriptName'))]"
      ]
    },
    "scriptArgs": {
      "value": null
    },
    "vnetName": {
      "value": "vnet1"
    },
    "vnetAddressSpace": {
      "value": "10.100.0.0/16"
    },
    "vnetSubnetName": {
      "value": "subnet1"
    },
    "vnetSubnetSpace": {
      "value": "10.100.1.0/24"
    },
    "nsgName": {
      "value": "nsg1"
    },
    "location": {
      "value": "[resourceGroup().location]"
    }
  }
}
